# Frontend production env example (CRA)
# Used at build time.

# API base URL for production (no trailing slash recommended)
# Example: https://api.calls.melamedlaw.co.il/api
REACT_APP_API_BASE_URL=__SET_ME__

# Global OTP enable flag for signing flows (public/client)
# false = hide OTP UI and block OTP endpoints
REACT_APP_SIGNING_OTP_ENABLED=false

# Default OTP policy when no explicit selection is made by lawyer
REACT_APP_SIGNING_REQUIRE_OTP_DEFAULT=false
